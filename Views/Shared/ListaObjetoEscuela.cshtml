@model IEnumerable<ObjetoEscuelaBase>

<table class="table table-dark table-hover">
    <thread>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thread>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Nombre</td>
                <td>@obj.Id</td>
            </tr>
        }
    </tbody>
</table>