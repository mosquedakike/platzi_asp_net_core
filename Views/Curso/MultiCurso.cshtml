@model IEnumerable<Curso>

@{
    ViewData["Title"] = "MultiCurso";
    Layout = "Simple";
}

<h1> @ViewData["Title"]</h1>

<strong>Desde la vista parcial</strong>




<table class="table table-dark table-hover">
<thread>
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
        <th scope="col">Jornada</th>
        <th scope="col">Dirección</th>
    </tr>
</thread>
<tbody>
    @foreach (var alumno in Model)
    {
    <tr>
        <td>@alumno.Nombre</td>
        <td>@alumno.Id</td>
        <td>@alumno.Jornada</td>
        <td>@alumno.Dirección</td>
    </tr>
    }
</tbody>
</table>

<p>@ViewBag.Fecha</p>